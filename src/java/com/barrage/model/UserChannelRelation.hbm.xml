<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.barrage.model.UserChannelRelation" table="user_channel_relation"
		dynamic-update="true" dynamic-insert="true">

		<id name="id" type="java.lang.Long" column="id">
			<generator class="native" />
		</id>

		<property name="userId" type="java.lang.Long" column="userId" />

		<property name="channelId" type="java.lang.Long" column="channelId" />

		<property name="status" type="java.lang.Long" column="status" />

	</class>
</hibernate-mapping>
