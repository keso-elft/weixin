<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.barrage.model.StoreMessage" table="store_message"
		dynamic-update="true" dynamic-insert="true">

		<id name="id" type="java.lang.Long" column="id">
			<generator class="native" />
		</id>

		<property name="userId" type="java.lang.Long" column="user_id" />

		<property name="channelId" type="java.lang.Long" column="channel_id" />

		<property name="content" type="java.lang.String" column="content" />

		<property name="sendTime" type="java.util.Date" column="send_time" />

	</class>
</hibernate-mapping>
